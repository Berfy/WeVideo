<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <we.video.wevideo.ui.support.refresh.Mater.MaterialRefreshLayout
        android:id="@+id/refreshView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:isLoadMore="true"
        app:overlay="true"
        app:progress_colors="@array/material_colors"
        app:progress_size_type="normal"
        app:wave_color="@color/translate"
        app:wave_height_type="normal"
        app:wave_show="true">

        <we.video.wevideo.ui.support.ScrollListView
            android:id="@+id/listView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:cacheColorHint="#00000000"
            android:divider="#00000000"></we.video.wevideo.ui.support.ScrollListView>

    </we.video.wevideo.ui.support.refresh.Mater.MaterialRefreshLayout>

</LinearLayout>